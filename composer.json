{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-ide-helper": "^2.4",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "symfony/process": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "doctrine/dbal": "~2.3",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laracasts/generators": "^1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "@php artisan migrate:fresh --force"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan migrate --force --seed",
            "yarn upgrade",
            "yarn run dev",
            "composer ide-helper"
        ],
        "reset-env":[
            "rm -rf node-modules",
            "rm -rf vendor/*/",
            "composer fix-silent",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "node_modules\\node-notifier\\vendor\\snoreToast\\SnoreToast.exe -t \"Reset your Development Environment\" -m \"Removed and reinstalled all packages, reset .env to .env.example and generated a new key.\" -p public\\images\\success-kid.jpg"

        ],
        "fix":[
            "composer fix-silent",
            "node_modules\\node-notifier\\vendor\\snoreToast\\SnoreToast.exe -t \"Composer Fix Succeded\" -m \"Updated to the latest common dependencies, everything should be safe and beautiful!\" -p public\\images\\success-kid.jpg",
            "yarn run dev"
        ],
        "fix-silent":[
            "composer install",
            "yarn install",
            "@php artisan migrate --force --seed",
            "composer ide-helper"
        ],
        "ide-helper":[
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:eloquent"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
